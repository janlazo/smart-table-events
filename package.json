{
  "name": "smart-table-events",
  "version": "1.0.3",
  "description": "tiny event emitter/listener",
  "main": "dist/",
  "jsnext:main": "index.js",
  "module": "dist/index.mjs",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "test": "xo ./index.js && (cat ./node_modules/zora/dist/zora.js && rollup -f iife -n test ./test/index.js) | node | tap-diff"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smart-table/smart-table-events.git"
  },
  "keywords": [
    "event",
    "events",
    "emitter",
    "listener",
    "smart-table"
  ],
  "author": "Laurent Renard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smart-table/smart-table-events/issues"
  },
  "homepage": "https://github.com/smart-table/smart-table-events#readme",
  "devDependencies": {
    "rollup": "^0.54.1",
    "tap-diff": "^0.1.1",
    "xo": "^0.18.2",
    "zora": "^1.1.6"
  }
}
